#include <stdio.h>
#include <stdlib.h>

int main()
{
    int num,i,t;
    scanf("%d", &num);
    int k=0;
    int arr[num],b[num];
    for(i = 0; i < num; i++) {
        scanf("%d", &arr[i]);
    }


    /* Write the logic to reverse the array. */
    for(i=num-1;i>=0;i--)
    {
        b[k]=arr[i];
        k++;
    }
   
    for(i = 0; i < num; i++)
            { arr[i]=b[i];
            printf("%d ",arr[i]);
            }
    return 0;
}
